				billboard.Name = "ImageBillboard"
				billboard.Size = UDim2.new(0, 200, 0, 200) -- fixed size
				billboard.Adornee = player.Character.Head
				billboard.AlwaysOnTop = true
				billboard.StudsOffset = Vector3.new(0, 0, 0) -- exactly at head position
				billboard.Parent = player.Character
	
				-- Prevent scaling with distance
				billboard.ExtentsOffset = Vector3.new(0, 0, 0)
				billboard.ClipsDescendants = false
				billboard.ResetOnSpawn = false
	
				-- Add ImageLabel
				local imageLabel = Instance.new("ImageLabel")
				imageLabel.Size = UDim2.new(1, 0, 1, 0)
				imageLabel.BackgroundTransparency = 1
				imageLabel.Image = "rbxassetid://104776703588210" -- replace with your image ID
				imageLabel.Parent = billboard
			end
		end
	end)
	
end;
