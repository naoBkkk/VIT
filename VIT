-- StarterGui.LoginGui.MainFrame.TextLabel.LocalScript
local function C_22()
local script = G2L["22"];
	-- LocalScript inside a TextLabel
	
	local textLabel = script.Parent
	local RunService = game:GetService("RunService")
	
	-- Ensure UIGradient exists
	local gradient = textLabel:FindFirstChildOfClass("UIGradient")
	if not gradient then
		gradient = Instance.new("UIGradient")
		gradient.Parent = textLabel
	end
	
	gradient.Rotation = 0
	
	-- Function to generate rainbow
	local function rainbowColors(hueShift)
		return ColorSequence.new{
			ColorSequenceKeypoint.new(0, Color3.fromHSV((0/6 + hueShift) % 1, 1, 1)),
			ColorSequenceKeypoint.new(0.2, Color3.fromHSV((1/6 + hueShift) % 1, 1, 1)),
			ColorSequenceKeypoint.new(0.4, Color3.fromHSV((2/6 + hueShift) % 1, 1, 1)),
			ColorSequenceKeypoint.new(0.6, Color3.fromHSV((3/6 + hueShift) % 1, 1, 1)),
			ColorSequenceKeypoint.new(0.8, Color3.fromHSV((4/6 + hueShift) % 1, 1, 1)),
			ColorSequenceKeypoint.new(1, Color3.fromHSV((5/6 + hueShift) % 1, 1, 1)),
		}
	end
	
	-- Animate (fast + perfect loop)
	local hue = 0
	RunService.RenderStepped:Connect(function(delta)
		hue = (hue + delta * 1) % 1 -- "1" makes it fast, adjust higher for faster
		gradient.Color = rainbowColors(hue)
	end)
	
end;
task.spawn(C_22);
-- StarterGui.LoginGui.MainFrame.TextButton.LocalScript
