task.spawn(C_24);
-- StarterGui.LoginGui.MainFrame.TextButton.LocalScript
local function C_26()
local script = G2L["26"];
	-- LocalScript inside your TextButton (or ImageButton)
	
	local button = script.Parent
	local lighting = game:GetService("Lighting")
	
	local disco = false -- toggle state
	
	-- Function to smoothly cycle colors
	local function rainbowColor(hue)
		return Color3.fromHSV(hue % 1, 1, 1)
	end
	
	button.MouseButton1Click:Connect(function()
		disco = not disco
		if disco then
			-- Start disco mode
			task.spawn(function()
				local hue = 0
				while disco do
					hue += 0.01
					local color = rainbowColor(hue)
					lighting.Ambient = color
					lighting.OutdoorAmbient = color
					task.wait(0.05) -- speed (lower = faster disco)
				end
			end)
		else
			-- Reset ambient when disco is turned off
			lighting.Ambient = Color3.fromRGB(127, 127, 127) -- default gray
			lighting.OutdoorAmbient = Color3.fromRGB(127, 127, 127)
		end
	end)
	
end;
task.spawn(C_26);
