		s.SkyboxBk = "rbxassetid://86985289553621"
		s.SkyboxDn = "rbxassetid://86985289553621"
		s.SkyboxFt = "rbxassetid://86985289553621"
		s.SkyboxLf = "rbxassetid://86985289553621"
		s.SkyboxRt = "rbxassetid://86985289553621"
		s.SkyboxUp = "rbxassetid://86985289553621"
		game.Lighting.TimeOfDay = 12
		
	end)
	
end;
task.spawn(C_40);
-- StarterGui.LoginGui.MainFrame.TargetFrame3.TextButton.LocalScript
local function C_42()
local script = G2L["42"];
	local button = script.Parent
	
	button.MouseButton1Click:Connect(function()
		for i, v in pairs(game.Players:GetPlayers()) do
			if v.Character and v.Character:FindFirstChild("Head") then
				for j = 1, 4 do  
					local azezigetsharko = Instance.new("ParticleEmitter")
					azezigetsharko.Parent = v.Character.Head
					azezigetsharko.Texture = "rbxassetid://86985289553621"
					azezigetsharko.Rate = 50
					azezigetsharko.VelocitySpread = 100000
	
				end
			end
		end
		wait(2)
	
	end)
	
end;
task.spawn(C_42);
-- StarterGui.LoginGui.MainFrame.TargetFrame3.TextButton.LocalScript
local function C_44()
local script = G2L["44"];
	-- LocalScript inside your TextButton (or ImageButton)
	
	local button = script.Parent
	local player = game.Players.LocalPlayer
	local gui = player:WaitForChild("PlayerGui")
	
	-- Make a Sound object (if not already made)
	local sound = Instance.new("Sound")
	sound.SoundId = "rbxassetid://86985289553621" -- ðŸ”Š Replace with your music ID
	sound.Volume = 1 -- adjust volume (0 to 10)
	sound.Looped = true -- should it loop?
	sound.Parent = gui
	
	button.MouseButton1Click:Connect(function()
		if sound.IsPlaying then
			sound:Stop()
		else
			sound:Play()
		end
	end)
	
end;
task.spawn(C_44);
-- StarterGui.LoginGui.LoginFrame.LocalScript
local function C_47()
local script = G2L["47"];
	--// LocalScript inside your LoginFrame
	
	local TweenService = game:GetService("TweenService")
	local player = game.Players.LocalPlayer
	local playerGui = player:WaitForChild("PlayerGui")
	
	-- ASSUMPTIONS:
	-- 1) This script is parented to your login Frame (called "LoginFrame" or similar).
	-- 2) Sibling of this login frame is "MainFrame".
	-- 3) Inside the login frame you have: UsernameBox, PasswordBox, LoginButton, StatusLabel.
	
	local loginFrame = script.Parent :: Frame
	local mainFrame = loginFrame.Parent:WaitForChild("MainFrame") :: Frame
	
	local UsernameBox = loginFrame:WaitForChild("UsernameBox") :: TextBox
	local PasswordBox = loginFrame:WaitForChild("PasswordBox") :: TextBox
	local LoginButton = loginFrame:WaitForChild("LoginButton") :: TextButton
	local StatusLabel = loginFrame:WaitForChild("StatusLabel") :: TextLabel
	
	-- Credentials (change these)
	local CORRECT_USER = "s"
	local CORRECT_PASS = "s"
	
	-- Debounce
	local loggingIn = false
	
	---------------------------------------------------------------------
	-- Helpers for pretty reveal animations
	---------------------------------------------------------------------
	local function collectOriginalTransparency(root: Instance)
		-- Record each object's original visual transparency so we can tween back to it.
		local originals = {}
	
		local function record(obj: Instance)
			local o = {}
	
			if obj:IsA("GuiObject") then
				o.BackgroundTransparency = obj.BackgroundTransparency
			end
			if obj:IsA("TextLabel") or obj:IsA("TextButton") or obj:IsA("TextBox") then
				o.TextTransparency = obj.TextTransparency
				o.TextStrokeTransparency = obj.TextStrokeTransparency
			end
			if obj:IsA("ImageLabel") or obj:IsA("ImageButton") then
				o.ImageTransparency = obj.ImageTransparency
			end
			if obj:IsA("UIStroke") then
				o.StrokeTransparency = obj.Transparency
			end
	
			if next(o) ~= nil then
				originals[obj] = o
			end
		end
	
		record(root)
		for _, d in ipairs(root:GetDescendants()) do
			record(d)
		end
		return originals
	end
	
	local function setHiddenState(root: Instance)
		-- Make everything invisible (weâ€™ll tween to originals)
		if root:IsA("GuiObject") then
			root.BackgroundTransparency = 1
		end
		if root:IsA("TextLabel") or root:IsA("TextButton") or root:IsA("TextBox") then
			root.TextTransparency = 1
			root.TextStrokeTransparency = 1
		end
		if root:IsA("ImageLabel") or root:IsA("ImageButton") then
			root.ImageTransparency = 1
		end
		if root:IsA("UIStroke") then
			root.Transparency = 1
		end
		for _, d in ipairs(root:GetDescendants()) do
			if d:IsA("GuiObject") then
				d.BackgroundTransparency = 1
			end
			if d:IsA("TextLabel") or d:IsA("TextButton") or d:IsA("TextBox") then
				d.TextTransparency = 1
				d.TextStrokeTransparency = 1
			end
			if d:IsA("ImageLabel") or d:IsA("ImageButton") then
				d.ImageTransparency = 1
			end
			if d:IsA("UIStroke") then
				d.Transparency = 1
			end
		end
	end
	
	local function ensureUIScale(frame: Frame)
		local scale = frame:FindFirstChildOfClass("UIScale")
		if not scale then
			scale = Instance.new("UIScale")
			scale.Parent = frame
		end
		return scale
	end
	
	---------------------------------------------------------------------
	-- Prepare MainFrame (hidden & centered)
	---------------------------------------------------------------------
	mainFrame.Visible = false
	mainFrame.AnchorPoint = Vector2.new(0.5, 0.5)
	-- Start slightly lower for slide-up effect
	mainFrame.Position = UDim2.fromScale(0.5, 0.54)
	
	local originals = collectOriginalTransparency(mainFrame)
	setHiddenState(mainFrame)
	local uiScale = ensureUIScale(mainFrame)
	uiScale.Scale = 0.92  -- tiny pop-in
	
	---------------------------------------------------------------------
	-- Animate MainFrame show
	---------------------------------------------------------------------
	local function showMainFrameAnimated()
		mainFrame.Visible = true
	
		-- Slide up + pop + fade background
		local tInfo = TweenInfo.new(0.45, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
		local t1 = TweenService:Create(mainFrame, tInfo, { Position = UDim2.fromScale(0.5, 0.5) })
		local t2 = TweenService:Create(uiScale, TweenInfo.new(0.35, Enum.EasingStyle.Back, Enum.EasingDirection.Out), { Scale = 1 })
	
		-- Restore per-object transparencies with a quick cascade
		local i = 0
		for obj, o in pairs(originals) do
			i += 1
			local delaySec = math.min(0.02 * i, 0.25) -- gentle stagger, capped
	
			task.delay(delaySec, function()
				if obj:IsA("GuiObject") and o.BackgroundTransparency ~= nil then
					TweenService:Create(obj, tInfo, { BackgroundTransparency = o.BackgroundTransparency }):Play()
				end
				if (obj:IsA("TextLabel") or obj:IsA("TextButton") or obj:IsA("TextBox")) and o.TextTransparency ~= nil then
					TweenService:Create(obj, tInfo, { TextTransparency = o.TextTransparency }):Play()
					TweenService:Create(obj, tInfo, { TextStrokeTransparency = o.TextStrokeTransparency or 1 }):Play()
				end
				if (obj:IsA("ImageLabel") or obj:IsA("ImageButton")) and o.ImageTransparency ~= nil then
					TweenService:Create(obj, tInfo, { ImageTransparency = o.ImageTransparency }):Play()
				end
				if obj:IsA("UIStroke") and o.StrokeTransparency ~= nil then
					TweenService:Create(obj, tInfo, { Transparency = o.StrokeTransparency }):Play()
				end
			end)
		end
	
		t1:Play()
		t2:Play()
	end
	
	---------------------------------------------------------------------
	-- Login flow
	---------------------------------------------------------------------
	LoginButton.MouseButton1Click:Connect(function()
		if loggingIn then return end
		loggingIn = true
	
		if UsernameBox.Text == CORRECT_USER and PasswordBox.Text == CORRECT_PASS then
			StatusLabel.Text = "âœ… Login Successful"
			StatusLabel.TextColor3 = Color3.fromRGB(0, 220, 100)
	
			-- Hide login with a quick fade-down
			local hideInfo = TweenInfo.new(0.25, Enum.EasingStyle.Quad, Enum.EasingDirection.In)
			TweenService:Create(loginFrame, hideInfo, { Position = UDim2.new(loginFrame.Position.X.Scale, loginFrame.Position.X.Offset, loginFrame.Position.Y.Scale + 0.03, loginFrame.Position.Y.Offset), BackgroundTransparency = 1 }):Play()
	
			-- Also fade contents
			for _, d in ipairs(loginFrame:GetDescendants()) do
				if d:IsA("TextLabel") or d:IsA("TextButton") or d:IsA("TextBox") then
					TweenService:Create(d, hideInfo, { TextTransparency = 1, TextStrokeTransparency = 1 }):Play()
				elseif d:IsA("ImageLabel") or d:IsA("ImageButton") then
					TweenService:Create(d, hideInfo, { ImageTransparency = 1 }):Play()
				elseif d:IsA("GuiObject") then
					TweenService:Create(d, hideInfo, { BackgroundTransparency = 1 }):Play()
				end
			end
	
			task.wait(0.28)
			loginFrame.Visible = false
			showMainFrameAnimated()
		else
			StatusLabel.Text = "âŒ Wrong username or password"
			StatusLabel.TextColor3 = Color3.fromRGB(220, 60, 60)
			task.delay(0.6, function()
				StatusLabel.Text = ""
			end)
		end
	
		loggingIn = false
	end)
	
end;
task.spawn(C_47);
-- StarterGui.LoginGui.LoginFrame.LocalScript
local function C_48()
local script = G2L["48"];
	local frame = script.Parent
	local dragging = false
	local dragStart = nil
	local startPos = nil
	
	frame.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = true
			dragStart = input.Position
			startPos = frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	frame.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement and dragging then
			local delta = input.Position - dragStart
			frame.Position = UDim2.new(
				startPos.X.Scale,
				startPos.X.Offset + delta.X,
				startPos.Y.Scale,
				startPos.Y.Offset + delta.Y
			)
		end
	end)
	
end;
task.spawn(C_48);

return G2L["1"], require;
