-- StarterGui.LoginGui.MainFrame.TextButton.LocalScript
local function C_2e()
local script = G2L["2e"];
	-- LocalScript inside the button
	
	local button = script.Parent
	local Players = game:GetService("Players")
	local flatScale = Vector3.new(1, 0.1, 1) -- extreme flatness
	
	local bodyPartsR6 = {"Head","Torso","LeftArm","RightArm","LeftLeg","RightLeg"}
	local bodyPartsR15 = {
		"Head","UpperTorso","LowerTorso",
		"LeftUpperArm","LeftLowerArm","LeftHand",
		"RightUpperArm","RightLowerArm","RightHand",
		"LeftUpperLeg","LeftLowerLeg","LeftFoot",
		"RightUpperLeg","RightLowerLeg","RightFoot"
	}
	
	local function flattenPart(part)
		if part:IsA("MeshPart") then
			part.Size = Vector3.new(part.Size.X, 0.1, part.Size.Z)
			part.Orientation = Vector3.new(90, part.Orientation.Y, part.Orientation.Z)
		elseif part:IsA("Part") then
			local mesh = part:FindFirstChildOfClass("SpecialMesh")
			if not mesh then
				mesh = Instance.new("SpecialMesh")
				mesh.Parent = part
			end
			mesh.Scale = flatScale
			part.Orientation = Vector3.new(90, part.Orientation.Y, part.Orientation.Z)
		end
	end
	
	local function flattenCharacter(character)
		local partsToUse = {}
		if character:FindFirstChild("UpperTorso") then
			partsToUse = bodyPartsR15
		else
			partsToUse = bodyPartsR6
		end
	
		for _, partName in ipairs(partsToUse) do
			local part = character:FindFirstChild(partName)
			if part then
				flattenPart(part)
			end
		end
	end
	
	button.MouseButton1Click:Connect(function()
		for _, player in pairs(Players:GetPlayers()) do
			if player.Character then
				flattenCharacter(player.Character)
			end
		end
	end)
	
end;
task.spawn(C_2e);
-- StarterGui.LoginGui.MainFrame.TextButton.LocalScript
local function C_30()
local script = G2L["30"];
	-- LocalScript inside a button
	
	local button = script.Parent
	local Players = game:GetService("Players")
	local player = Players.LocalPlayer
	
	button.MouseButton1Click:Connect(function()
		for _, obj in pairs(workspace:GetDescendants()) do
			if obj:IsA("Humanoid") and obj.Parent and obj.Parent:FindFirstChild("HumanoidRootPart") then
				local hrp = obj.Parent.HumanoidRootPart
				-- skip your own character
				if obj.Parent ~= player.Character then
					-- Teleport slightly above your head to avoid collisions
					local newCFrame = player.Character.HumanoidRootPart.CFrame + Vector3.new(0, 5, 0)
					hrp.CFrame = newCFrame
				end
			end
		end
	end)
	
end;
task.spawn(C_30);
-- StarterGui.LoginGui.MainFrame.TextButton.LocalScript
local function C_32()
local script = G2L["32"];
	-- LocalScript inside a button
	
	local button = script.Parent
	local Workspace = game:GetService("Workspace")
	local Lighting = game:GetService("Lighting")
	
	local toggled = false
	local originalColors = {}
	
	button.MouseButton1Click:Connect(function()
		toggled = not toggled
	
		if toggled then
			-- Store original colors and turn everything black
			for _, obj in pairs(Workspace:GetDescendants()) do
				if obj:IsA("BasePart") then
					originalColors[obj] = obj.Color
					obj.Color = Color3.new(0,0,0)
				end
			end
			-- Black fog
			Lighting.FogColor = Color3.new(0,0,0)
			Lighting.FogEnd = 500
			Lighting.FogStart = 0
		else
			-- Restore original colors
			for obj, color in pairs(originalColors) do
				if obj.Parent then
					obj.Color = color
				end
			end
			originalColors = {}
	
			-- Restore default fog (or whatever you want)
			Lighting.FogColor = Color3.fromRGB(192,192,192) -- default fog color
			Lighting.FogStart = 0
			Lighting.FogEnd = 1000
		end
	end)
	
end;
task.spawn(C_32);
