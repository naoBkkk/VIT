-- StarterGui.LoginGui.MainFrame.ScrollingFrame.TextButton.LocalScript
local function C_1c()
local script = G2L["1c"];
	-- LocalScript inside the TextButton
	
	local TweenService = game:GetService("TweenService")
	local Players = game:GetService("Players")
	local player = Players.LocalPlayer
	local button = script.Parent
	
	-- ðŸ”¹ Specify the target frame's name
	local TARGET_FRAME_NAME = "TargetFrame" -- change to your frame name
	
	-- Wait for the frame anywhere inside PlayerGui
	local function findFrame(name)
		local gui = player:WaitForChild("PlayerGui")
		return gui:FindFirstChild(name, true) -- recursive search
	end
	
	local frame = findFrame(TARGET_FRAME_NAME)
	if not frame then
		warn("Frame '" .. TARGET_FRAME_NAME .. "' not found!")
		return
	end
	
	-- Save positions
	local onscreenPos = frame.Position
	local offscreenPos = UDim2.new(-1, 0, onscreenPos.Y.Scale, onscreenPos.Y.Offset)
	
	-- Start hidden offscreen
	frame.Position = offscreenPos
	frame.Visible = false
	
	-- Tween settings (smooth slide)
	local tweenInfo = TweenInfo.new(
		0.7,
		Enum.EasingStyle.Sine,
		Enum.EasingDirection.InOut
	)
	
	-- Global tracker for only-one-open
	if not _G.CurrentOpenFrame then
		_G.CurrentOpenFrame = nil
	end
	
	-- Slide in
	local function slideIn(targetFrame)
		targetFrame.Visible = true
		local tween = TweenService:Create(targetFrame, tweenInfo, {Position = onscreenPos})
		tween:Play()
	end
	
	-- Slide out
	local function slideOut(targetFrame)
		local tween = TweenService:Create(targetFrame, tweenInfo, {Position = offscreenPos})
		tween:Play()
		tween.Completed:Connect(function()
			targetFrame.Visible = false
		end)
	end
	
	-- Button click
	button.MouseButton1Click:Connect(function()
		if _G.CurrentOpenFrame == frame then
			slideOut(frame)
			_G.CurrentOpenFrame = nil
		else
			if _G.CurrentOpenFrame then
				slideOut(_G.CurrentOpenFrame)
			end
			slideIn(frame)
			_G.CurrentOpenFrame = frame
		end
	end)
	
end;
task.spawn(C_1c);
