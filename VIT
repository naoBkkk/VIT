-- StarterGui.LoginGui.MainFrame.LocalScript
local function C_3()
local script = G2L["3"];
	local frame = script.Parent
	local dragging = false
	local dragStart = nil
	local startPos = nil
	
	frame.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = true
			dragStart = input.Position
			startPos = frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	frame.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement and dragging then
			local delta = input.Position - dragStart
			frame.Position = UDim2.new(
				startPos.X.Scale,
				startPos.X.Offset + delta.X,
				startPos.Y.Scale,
				startPos.Y.Offset + delta.Y
			)
		end
	end)
	
end;
task.spawn(C_3);
-- StarterGui.LoginGui.MainFrame.TargetFrame.TextButton.LocalScript
local function C_7()
local script = G2L["7"];
	-- LocalScript inside a TextButton
	local button = script.Parent
	local decalID = "rbxassetid://115422007619445" -- replace with your decal ID
	
	local function spamDecalsOnAllParts()
		for _, obj in ipairs(workspace:GetDescendants()) do
			if obj:IsA("BasePart") then
				-- Remove all existing decals and textures
				for _, child in ipairs(obj:GetChildren()) do
					if child:IsA("Decal") or child:IsA("Texture") then
						child:Destroy()
					end
				end
	
				-- Apply your decal to all six faces
				for _, face in pairs(Enum.NormalId:GetEnumItems()) do
					local newDecal = Instance.new("Decal")
					newDecal.Texture = decalID
					newDecal.Face = face
					newDecal.Parent = obj
				end
			end
		end
